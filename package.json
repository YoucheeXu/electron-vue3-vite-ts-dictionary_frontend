{
  "name": "electron-vue3-vite-ts-dictionary",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "kill-port 3920 && concurrently -k \"vite\" \"wait-on tcp:3920 && npm-run-all watch\"",
    "watch": "tsc-watch --noClear -p tsconfig.app.json --onSuccess \"npm-run-all start:ect\"",
    "start:ect": "cross-env NODE_ENV=development electron ./output/build/electron/background.js",
    "dev": "vite",
    "test:unit": "vitest",
    "build-only": "tsc && vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "oxlint && eslint",
    "lint:fix": "oxlint --fix && eslint --fix",
    "format": "prettier --write src/",
    "build": "npm-run-all build:vue build:tsc build:win",
    "build:vue": "vue-tsc --noEmit && vite build",
    "build:tsc": "tsc -p tsconfig.app.json",
    "build:all": "electron-builder --mac --windows",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --windows",
    "preview": "cross-env NODE_ENV=production electron ./output/build/electron/background.js"
  },
  "author": "Youchee Xu",
  "license": "MIT",
  "build": {
    "appId": "Youchee.Dictionary",
    "asar": true,
    "productName": "Dictionary",
    "directories": {
      "output": "output/app"
    },
    "win": {
      "icon": "public/duck.jpg",
      "target": [
        "dir"
      ]
    },
    "files": [
      "output/**/*"
    ],
    "mac": {
      "category": "public.app-category.utilities.test",
      "icon": "public/duck.jpg"
    }
  },
  "dependencies": {
    "adler-32": "^1.3.1",
    "axios": "^1.13.1",
    "element-plus": "^2.8.1",
    "fast-xml-parser": "^4.4.1",
    "jszip": "^3.10.1",
    "log4js": "^6.9.1",
    "lzo-decompress": "^1.0.11",
    "pinia": "^2.1.7",
    "sqlite3": "5.1.7",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^36.1.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-oxlint": "^0.16.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "jsdom": "^24.1.0",
    "kill-port": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "npm-run-all2": "^6.2.0",
    "oxlint": "^0.16.3",
    "prettier": "3.4.2",
    "tsc-watch": "^6.2.0",
    "typescript": "~5.4.0",
    "typescript-eslint": "^8.19.0",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21",
    "wait-on": "^8.0.0"
  },
  "main": "./output/build/electron/background.js"
}
